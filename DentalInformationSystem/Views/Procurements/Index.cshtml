@model IEnumerable<DentalInformationSystem.Models.Procurement>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProcurmentDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Supplier)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryNoteNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MerchandiseName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MeasureUnit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Quantity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalPrice)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProcurmentDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Supplier.AccountNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryNoteNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MerchandiseName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MeasureUnit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalPrice)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ProcurmentID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ProcurmentID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ProcurmentID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
